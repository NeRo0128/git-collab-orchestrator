{
  "name": "git-collab-orchestrator",
  "version": "1.0.0",
  "description": "CLI to orchestrate multiple AI agents working in parallel on a Git project",
  "type": "module",
  "bin": {
    "gco": "./bin/gco.js"
  },
  "main": "./src/index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ bin/ tests/",
    "format": "prettier --write 'src/**/*.js' 'bin/**/*.js' 'tests/**/*.js'",
    "format:check": "prettier --check 'src/**/*.js' 'bin/**/*.js' 'tests/**/*.js'"
  },
  "keywords": [
    "git",
    "collaboration",
    "ai",
    "agents",
    "orchestrator",
    "cli",
    "copilot",
    "tasks"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "date-fns": "^3.6.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.2.0",
    "marked": "^12.0.0",
    "marked-terminal": "^7.0.0",
    "simple-git": "^3.24.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.2.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
